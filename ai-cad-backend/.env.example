# Example environment variables for AI CAD Project
# Copy this file to `.env` and replace placeholders with real values

# Hugging Face API key (get from https://huggingface.co/settings/tokens)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# OpenAI API key (optional, if using OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# FastAPI secret (for session security/JWT)
SECRET_KEY=your_secret_here

# Database URL (optional, if you add a database later)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
